{
  "manifest_version": 2,
  "version": "0.3.3.2",
  "name": "Block twitter and reddit",
  "description": "blocks twitter and reddit cz that's why",
  "content_scripts": [{
    "matches": [
      "*://*.twitter.com/*",
      "*://*.reddit.com/*"
    ],
    "js": [
      "inject.js"
    ],
    "run_at": "document_start",
    "all_frames": false
  }],
  "icons": {
    "16": "icons/16.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/16.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
    }
  }
}
